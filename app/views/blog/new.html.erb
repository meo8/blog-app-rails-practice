<h1>Add new blog</h1>
<%= form_with url: create_blog_path, local: true do %>
    <label for='title'>Blog title</label>
    <input type='text' name='title' />

    <br />

    <label for="content">Blog content</label>
    <input type="text" name="content">

    <br />
    <input type="submit" name="submit">
<% end %>
